<h1>Courses#show</h1>
<p>Find me in app/views/courses/show.html.erb</p>


<%= render partial:"partials/breadcrumbs", locals:{breadCrumbHash:{"Home"=>root_path, "Courses"=>courses_path, (@course.subject.abbr+@course.toSingleCode)=>'#'}} %>

<div class="container">
	<div class="page-header">
		<h1 class="offset1"><%= @course.name %></h1>
	</div>
	<div class="tabbable"> <!-- Only required for left/right tabs -->
		<ul class="nav nav-pills">
			<li class="active">
				<a href="#tab1" data-toggle="tab">Overview</a>
			</li>
		</ul>
		<div class="tab-content">
			<div class="tab-pane fade in active" id="tab1">
				<%= render 'courses/overview'%>
			</div>

		</div>
	</div>

</div>
