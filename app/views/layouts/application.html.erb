<!DOCTYPE html>
<html>
<head>
  <title>Welcome to Nubhub</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= stylesheet_link_tag	"http://fonts.googleapis.com/css?family=Cabin:700" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
 <%= render 'layouts/shim' %>
 <%= yield :specific_assets %>
</head>
<body>
	<% if !current_page?('/') %>
		<%= render 'layouts/header' %>
	<% end %>
	<%= yield %>
	<% if !current_page?('/') %>
		<%= render 'layouts/footer' %>
	<% end %>
</body>
</html>
