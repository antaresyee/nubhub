
<div class="row-fluid">
	<div class="span12">
		<%= render 	partial:"partials/breadcrumbs", 
				locals:{
					breadCrumbHash:{
						"Home" => root_path, 
						"Browse" => browser_index_path}
					} %>
	</div>
</div>

<div id="nav">
	<a href="#test1" class="selected">One</a>
	<a href="#test2">Two</a>
	<a href="#test3">Three</a>
	<a href="#test4">Four</a>
</div>


<div class="row-fluid">
	<div class="span12">
		<div class="navbar">
			<div class="navbar-inner">
				<div class="brand">
					Browse: 
				</div>
				<ul class="nav hashchange">
					<li class="active" id="courses">
						<%= link_to "Courses", 
							"#courses", {class: 'sliderLink',
								path: 'courses'} %>
					</li>
					<li id="notes">
						<%= link_to "Notes", 
							'#notes', {class: 'sliderLink', 
								path: 'notes'} %>

					</li>
				</ul>
			</div>
		</div>
	</div>
</div>

<div id="slider">
	<%= render "courses/index" %>
</div>
<!--
-->

    <div class="content_container" id="courses_container">
    <div class="content_header" id="courses_header">
      <p id="courses_name">course</p>
    </div>
    <div class="content_box" id="courses_box">
      <div class="note_line">
        <ul>
          <li class="courses_line_name"><a>108: Software Design and Implementation</a></li>
        </ul>
        <%= image_tag("course_icon.jpg", width: 15, height: 19, class: "course_icon")%>
        <%= image_tag("plus_icon_2.jpg", width: 15, height: 19, class: "plus_icon")%>
        <%= image_tag("x_icon.jpg", width: 15, height: 19, class: "x_icon")%>
      </div>
      <div class="note_line">
        <ul>
          <li class="courses_line_name"><a>110: Operating Systems</a></li>
        </ul>
        <%= image_tag("course_icon.jpg", width: 15, height: 19, class: "course_icon")%>
        <%= image_tag("plus_icon_2.jpg", width: 15, height: 19, class: "plus_icon")%>
        <%= image_tag("x_icon.jpg", width: 15, height: 19, class: "x_icon")%>
      </div>
      <div class="note_line"></div>
      <div class="note_line"></div>
      <div class="note_line"></div>
    </div>
  </div>

  <div class="content_container" id="notes_container">
    <div class="content_header" id="notes_header">
      <p id="notes_date">date</p>
      <p id="notes_author">author</p>
    </div>
    <div class="content_box" id="notes_box">
      <div class="note_line">
        <ul id="note_line_ul">
          <li class="note_line_date">2/24/12</li>
          <li class="note_line_author">antaresyee</li>
          <li class="note_line_downloads">18</li>
        </ul>
        <%= image_tag("note_icon.jpg", width: 14, height: 18, class: "note_icon")%>
        <%= image_tag("download_icon.jpg", width: 20, height: 20, class: "download_jkicon")%>
      </div>
      <div class="note_line"></div>
      <div class="note_line"></div>
      <div class="note_line"></div>
      <div class="note_line"></div>
    </div>
  </div>




<div class="container">
	<div class="page-header">
		<h1 class="offset1"><%= @course.name %></h1>
	</div>
	<div class="tabbable"> <!-- Only required for left/right tabs -->
		<ul class="nav nav-pills">
			<li class="active">
				<a href="#tab1" data-toggle="tab">Overview</a>
			</li>
		</ul>
		<div class="tab-content">
			<div class="tab-pane fade in active" id="tab1">
				<%= render 'courses/overview'%>
			</div>

		</div>
	</div>

</div>



