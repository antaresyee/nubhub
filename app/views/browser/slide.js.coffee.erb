jQuery ->
  $(window).hashchange ->
    $('.test').html('OKAYAJ')
    hash = location.hash
    temp = document.createElement("a")
    $('.hashchange li a').each ->
      that = $(this)
      par = that.parent('li')

      if par.hasClass('active')
        par.removeClass('active')
      else if that.attr('href') == hash
        par.addClass('active')
        $(temp).attr("path", that.attr("path"))

    content = '<%= j(render partial: "notes/index") %>'
    $('#slider').html(content)